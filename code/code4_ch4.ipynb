    "import sympy as sp\n",
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "x = sp.Symbol('x')\n",
    "\n",
    "# Step 1: Define the function\n",
    "g = x**4 - 4*x**2\n",
    "\n",
    "# Step 2: Compute the second derivative\n",
    "g_double = sp.diff(g, x, 2)\n",
    "print(\"Second derivative:\", g_double)\n",
    "\n",
    "# Step 3: Solve g''(x)=0 for inflection point candidates\n",
    "inflection_candidates = sp.solve(sp.Eq(g_double, 0), x)\n",
    "print(\"Candidates:\", inflection_candidates)\n",
    "\n",
    "# Step 4: Check concavity by plugging simple points left/right\n",
    "for c in inflection_candidates:\n",
    "    left_val = g_double.subs(x, c - 1)\n",
    "    right_val = g_double.subs(x, c + 1)\n",
    "    print(f\"At x={c}, g''(x-1)={left_val}, g''(x+1)={right_val}\")\n",
    "    if sp.sign(left_val) != sp.sign(right_val):\n",
    "        print(f\"--> x={c} is an inflection point!\")\n",
    "    else:\n",
    "        print(f\"--> x={c} is NOT an inflection point\")\n",
    "\n",
    "# Step 5: Plot function and mark candidates\n",
    "g_func = sp.lambdify(x, g, 'numpy')\n",
    "xx = np.linspace(-3, 3, 400)\n",
    "yy = g_func(xx)\n",
    "\n",
    "plt.plot(xx, yy, label=\"g(x)\")\n",
    "for c in inflection_candidates:\n",
    "    cx = float(c)\n",
    "    plt.plot(cx, g_func(cx), 'ro')\n",
    "    plt.text(cx, g_func(cx)+1, f\"x={cx}\", ha='center')\n",
    "plt.title(\"Concavity and Inflection Check\")\n",
    "plt.legend()\n",
    "plt.show()\n"
   ]
