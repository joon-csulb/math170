    "f = x**3 - 3*x**2 + 2\n",
    "f_prime = sp.diff(f, x)\n",
    "f_double = sp.diff(f_prime, x)\n",
    "\n",
    "# 1. Solve f'(x) = 0\n",
    "crit_eq0 = sp.solve(sp.Eq(f_prime, 0), x)\n",
    "\n",
    "# 2. Check where derivative is undefined\n",
    "domain = sp.calculus.util.continuous_domain(f, x, sp.S.Reals)\n",
    "prime_domain = sp.calculus.util.continuous_domain(f_prime, x, sp.S.Reals)\n",
    "crit_undef = list(domain - prime_domain)\n",
    "\n",
    "critical_points = sorted(set(crit_eq0 + crit_undef))\n",
    "print(\"Critical points:\", critical_points)\n",
    "\n",
    "# Classify with second derivative\n",
    "print([(c, f_double.subs(x, c), f.subs(x, c)) for c in critical_points])\n",
    "\n",
    "# Plot with critical points\n",
    "ax = plot_function(f, x_min=-1, x_max=4, n=400, label='$f(x)$')\n",
    "f_np = f_to_numpy(f)\n",
    "for c in critical_points:\n",
    "    cx = float(c)\n",
    "    ax.plot(cx, f_np(cx), 'ro')\n",
    "    ax.text(cx, f_np(cx)+1, f'x={sp.N(c)}', ha='center')\n",
    "ax.set_title(\"Critical Points and Function Profile\")\n",
    "plt.show()\n",
    "\n",
