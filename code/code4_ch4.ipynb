    "import sympy as sp\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from scipy.optimize import minimize_scalar, minimize\n",
    "\n",
    "sp.init_printing(use_latex=True)  # pretty printing\n",
    "\n",
    "x = sp.Symbol('x', real=True)\n",
    "\n",
    "# Utility: lambdify safely for NumPy vectorization\n",
    "def f_to_numpy(expr, vars=(x,)):\n",
    "    \"\"\"Return a NumPy-ready function from a SymPy expression.\"\"\"\n",
    "    return sp.lambdify(vars, expr, modules=['numpy'])\n",
    "\n",
    "# Plot helpers\n",
    "def plot_function(expr, x_min=-5, x_max=5, n=400, label=None, ax=None):\n",
    "    \"\"\"Plot a 1D SymPy expression over [x_min, x_max].\"\"\"\n",
    "    if ax is None:\n",
    "        fig, ax = plt.subplots()\n",
    "    f_np = f_to_numpy(expr)\n",
    "    X = np.linspace(x_min, x_max, n)\n",
    "    Y = f_np(X)\n",
    "    ax.plot(X, Y, label=label if label else sp.sstr(expr))\n",
    "    ax.axhline(0, linewidth=0.8, color='k')\n",
    "    ax.axvline(0, linewidth=0.8, color='k')\n",
    "    ax.grid(True, linestyle=':')\n",
    "    if label:\n",
    "        ax.legend()\n",
    "    return ax"
   ]
