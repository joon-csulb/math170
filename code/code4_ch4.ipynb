    "import sympy as sp\n",
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "x = sp.Symbol('x')\n",
    "\n",
    "# Define function and derivative\n",
    "f = x**3 - 2*x - 5\n",
    "f_prime = sp.diff(f, x)\n",
    "\n",
    "f_np = sp.lambdify(x, f, 'numpy')\n",
    "f1_np = sp.lambdify(x, f_prime, 'numpy')\n",
    "\n",
    "# Initial guess\n",
    "x0 = 2.0\n",
    "\n",
    "# Step-by-step iterations\n",
    "x1 = x0 - f_np(x0)/f1_np(x0)\n",
    "x2 = x1 - f_np(x1)/f1_np(x1)\n",
    "x3 = x2 - f_np(x2)/f1_np(x2)\n",
    "x4 = x3 - f_np(x3)/f1_np(x3)\n",
    "x5 = x4 - f_np(x4)/f1_np(x4)\n",
    "\n",
    "xs = [x0, x1, x2, x3, x4, x5]\n",
    "print(\"Newton iterates (step-by-step):\", xs)\n",
    "\n",
    "# Plot\n",
    "xx = np.linspace(-3, 3, 400)\n",
    "yy = f_np(xx)\n",
    "\n",
    "plt.plot(xx, yy, label=\"f(x)\")\n",
    "plt.axhline(0, color='black', linewidth=0.8)\n",
    "plt.plot(xs, [f_np(v) for v in xs], 'o--', label=\"Newton iterates\")\n",
    "plt.legend()\n",
    "plt.title(\"Newton's Method (step-by-step)\")\n",
    "plt.grid(True, linestyle=':')\n",
    "plt.show()\n"
   ]
